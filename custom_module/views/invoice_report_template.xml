<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <template id="source_report_invoice_document" inherit_id="account.report_invoice_document_with_payments">
            <xpath expr="//t[@t-foreach='payments_vals']" position='replace'>
                    <t t-foreach="payments_vals" t-as="payment_vals">
                    <tr>
                        <td>
                            <i class="oe_form_field text-right oe_payment_label">Paid on <t t-esc="payment_vals['date'].strftime('%d-%m-%Y')"/></i>
                        </td>
                        <td class="text-right">
                            <span t-esc="payment_vals['amount']" t-options='{"widget": "monetary", "display_currency": o.currency_id}'/>
                        </td>
                    </tr>
                </t>
            </xpath>
        </template>
        <template id="custom_report_invoice_document" inherit_id="account.report_invoice_document">

            <!--            <xpath expr='//t[@t-set="address"]' position='after'>-->
            <!--                <t t-set="address">-->
            <!--                    <address t-field="o.partner_id" t-options='{"widget": "contact", "fields": ["address", "name"], "no_marker": True}' />-->
            <!--                    <div t-if="o.partner_id.vat" class="mt16"><t t-esc="o.company_id.country_id.vat_label or 'Tax ID'"/>: <span t-field="o.partner_id.vat"/></div>-->
            <!--                </t>-->
            <!--            </xpath>-->

            <xpath expr='//div[@name="reference"]' position='replace'>
                <!--                                    <div class="col-auto mw-100 mb-2" t-if="o.sltech_months" name="sltech_months">-->
                <!--                                        <strong>Month:</strong>-->
                <!--                                        <p class="m-0" t-field="o.sltech_months"/>-->
                <!--                                    </div>-->
                <!--                                    <div class="col-auto mw-100 mb-2" t-if="o.sltech_years" name="sltech_years">-->
                <!--                                        <strong>Year:</strong>-->
                <!--                                        <p class="m-0" t-field="o.sltech_years"/>-->
                <!--                                    </div>-->
            </xpath>

            <xpath expr="//table[@name='invoice_line_table']" position='replace'>
                <table class="table table-sm o_main_table" name="invoice_line_table"
                       style="border-bottom:3px solid #2e549a !important;">
                    <thead>
                        <tr>
                            <!-- TODO: remove in master -->
                            <t t-set="colspan" t-value="6"/>
                            <th name="th_description" class="text-left"
                                style="color:#2e549a !important;border-top:3px solid #2e549a !important;">
                                <span>Description</span>
                            </th>
                            <!--                            <th name="th_source" class="d-none text-left">-->
                            <!--                                <span>Source Document</span>-->
                            <!--                            </th>-->
                            <th name="th_quantity" class="text-right"
                                style="color:#2e549a !important;border-top:3px solid #2e549a !important;">
                                <span>Unit</span>
                            </th>
                            <!--                                <th name="th_rate" class="text-right"><span>Rate</span></th>-->
                            <th name="th_priceunit"
                                style="color:#2e549a !important;border-top:3px solid #2e549a !important;"
                                t-attf-class="text-right {{ 'd-none d-md-table-cell' if report_type == 'html' else '' }}">
                                <span>Amount</span>
                            </th>
                            <th name="th_price_unit" t-if="display_discount"
                                style="color:#2e549a !important;border-top:3px solid #2e549a !important;"
                                t-attf-class="text-right {{ 'd-none d-md-table-cell' if report_type == 'html' else '' }}">
                                <span>Disc.(%)</span>
                                <!-- TODO: remove in master -->
                                <t t-set="colspan" t-value="colspan+1"/>
                            </th>
                            <th name="th_taxes"
                                style="color:#2e549a !important;border-top:3px solid #2e549a !important;"
                                t-attf-class="text-right {{ 'd-none d-md-table-cell' if report_type == 'html' else '' }}">
                                <span>GST</span>
                            </th>
                            <th name="th_subtotal" class="text-right"
                                style="color:#2e549a !important;border-top:3px solid #2e549a !important;">
                                <span groups="account.group_show_line_subtotals_tax_excluded">Net Amount</span>
                                <span groups="account.group_show_line_subtotals_tax_included">Total Price</span>
                            </th>
                        </tr>
                    </thead>
                    <tbody class="invoice_tbody">
                        <t t-set="current_subtotal" t-value="0"/>


                        <tr>
                            <td name="account_invoice_line_name">
                                <span t-field="o.consolidated_invoice_line"/>
                            </td>
                            <td class="text-right">
                                <span t-esc="1"/>
                            </td>
                            <td t-attf-class="text-right {{ 'd-none d-md-table-cell' if report_type == 'html' else '' }}">
                                <span t-field="o.amount_untaxed"/>
                            </td>
                            <td t-if="display_discount"
                                t-attf-class="text-right {{ 'd-none d-md-table-cell' if report_type == 'html' else '' }}">
                            </td>
                            <td t-attf-class="text-right {{ 'd-none d-md-table-cell' if report_type == 'html' else '' }}">
                                <span t-field="o.amount_tax"/>
                            </td>
                            <td class="text-right o_price_total">
                                <span t-field="o.amount_untaxed"
                                      groups="account.group_show_line_subtotals_tax_excluded"/>
                            </td>
                        </tr>
                        <tr class="alternet_color">
                            <td style="border:none;">
                                <!--                                                <t t-esc="row.date"/>-->
                                <b style="text-decoration:underline;">FOR THE MONTH OF:</b>
                                <br/>
                                <b>
                                    <t t-esc="dict(o._fields['sltech_months'].selection).get(o.sltech_months)"/>
                                </b>
                                &amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;
                                <b>
                                    <t t-esc="o.sltech_years"/>
                                </b>
                            </td>
                            <td style="border:none;"></td>
                            <td style="border:none;"></td>
                            <td style="border:none;"></td>
                            <td style="border:none;"></td>
                        </tr>
                        <tr class="alternet_color">
                            <td style="border:none;">
                                <t t-esc="o.consolidated_invoice_note"/>
                                <br/>
                            </td>
                            <td style="border:none;"></td>
                            <td style="border:none;"></td>
                            <td style="border:none;"></td>
                            <td style="border:none;"></td>
                        </tr>
                    </tbody>
                </table>
            </xpath>
            <xpath expr="//div[hasclass('clearfix')]" position='replace'>
                <div class="clearfix">
                    <div id="total" class="row">
                        <div t-attf-class="#{'col-4' if report_type != 'html' else 'col-sm-7 col-md-5'} ml-auto">
                            <table class="table table-sm" style="page-break-inside: avoid;">
                                <tr class="border-#fff o_subtotal" style="border-bottom:1px solid #2e549a !important;">
                                    <td style="border-bottom:1px solid #2e549a !important;">
                                        <strong>Total (Ex GST)</strong>
                                    </td>
                                    <td style="border-bottom:1px solid #2e549a !important;" class="text-right">
                                        <span t-field="o.amount_untaxed"/>
                                    </td>
                                </tr>
                                <t t-foreach="o.amount_by_group" t-as="amount_by_group">
                                    <tr>
                                        <t t-if="len(o.tax_line_ids) == 1 and o.amount_untaxed == amount_by_group[2]">
                                            <td style="border-bottom:1px solid #2e549a !important;">
                                                <strong t-esc="amount_by_group[0]"/>
                                            </td>
                                            <td style="border-bottom:1px solid #2e549a !important;"
                                                class="text-right o_price_total">
                                                <span t-esc="amount_by_group[3]"/>
                                            </td>
                                        </t>
                                        <t t-else="">
                                            <td style="border-bottom:1px solid #2e549a !important;">
                                                <strong t-esc="amount_by_group[0]"/>
                                                <span>&amp;nbsp;
                                                    <span>on</span>
                                                    <t t-esc="amount_by_group[4]"/>
                                                </span>
                                            </td>
                                            <td style="border-bottom:1px solid #2e549a !important;"
                                                class="text-right o_price_total">
                                                <span t-esc="amount_by_group[3]"/>
                                            </td>
                                        </t>
                                    </tr>
                                </t>
                                <tr class="border-#fff o_total" style="border-top:1px solid #2e549a !important;">
                                    <td style="border-top:1px solid #2e549a !important;border-bottom:3px solid #2e549a !important;">
                                        <strong>Total Amount Payable</strong>
                                    </td>
                                    <td style="border-top:1px solid #2e549a !important;border-bottom:3px solid #2e549a !important;"
                                        class="text-right">
                                        <span t-field="o.amount_total"/>
                                    </td>
                                </tr>
                                <t t-set="payments_vals" t-value="o._get_payments_vals()"/>
                                <t t-foreach="payments_vals" t-as="payment_vals">
                                    <tr>
                                        <td>
                                            <i class="oe_form_field text-right oe_payment_label">Paid on
                                                <t t-esc="payment_vals['date'].strftime('%d-%m-%Y')"/>
                                            </i>
                                        </td>
                                        <td class="text-right">
                                            <span t-esc="payment_vals['amount']"
                                                  t-options='{"widget": "monetary", "display_currency": o.currency_id}'/>
                                        </td>
                                    </tr>
                                </t>
                                <t t-if="len(payments_vals) > 0">
                                    <tr class="border-black">
                                        <td>
                                            <strong>Amount Due</strong>
                                        </td>
                                        <td class="text-right">
                                            <span t-field="o.residual"/>
                                        </td>
                                    </tr>
                                </t>
                            </table>
                        </div>
                    </div>
                </div>

            </xpath>

        </template>


        <template id="custom_report_invoice_with_payments">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="o">
                    <t t-set="lang"
                       t-value="o.user_id.sudo().lang if o.type in ('in_invoice', 'in_refund') else o.partner_id.lang"/>

                    <t t-call="custom_module.custom_report_invoice_document" t-lang="lang"/>
                </t>
            </t>
        </template>

