            </field>
            <!--<field name="view_id" eval="helpdesk_team_tree_views" />-->
        </record>

        <record id="invoice_line_service_tickets_tree" model="ir.ui.view">
            <field name="name">helpdesk.invoice.line.views.tree</field>
            <field name="model">helpdesk.invoice.line</field>
            <!--<field name="inherit_id" ref="gt_helpdesk_support_ticket.helpdesk_ticket_tree_views"/>-->
            <field name="arch" type="xml">
                <tree string="ServiceDesk Invoice Line" create="false" delete="false"
                      decoration-danger="status == True">
                    <field name="helpdesk_inv_ids"/>
                    <field name="ticket_title"/>
                    <field name="product_id"/>
                    <field name="description"/>
                    <field name="ac_analytic_id"/>
                    <field name="qty"/>
                    <field name="uom"/>
                    <field name="resolution"/>
<!--                    <field name="unit_price"/>-->
                    <field name="inv_tax"/>

                    <field name="sltech_net_amount"/>
                    <field name="tax_amt"/>

                    <field name="amt"/>
                    <field name="ticket_create_date"/>
                    <field name="status" invisible="1"/>
                </tree>
            </field>
        </record>

        <record id="helpdesk_invoice_line_view_search" model="ir.ui.view">
        <field name="name">helpdesk.invoice.line.view</field>
        <field name="model">helpdesk.invoice.line</field>
        <field name="arch" type="xml">
            <search string="ServiceDesk Invoice Line">
                <group expand="0" string="Group By">
                    <filter name="Ticket Title" string="Ticket Title" domain="[]" context="{'group_by':'ticket_title'}"/>
                </group>
            </search>
        </field>
    </record>

